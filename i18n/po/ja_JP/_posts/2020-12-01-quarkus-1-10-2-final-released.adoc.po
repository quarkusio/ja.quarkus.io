msgid "`quarkus.hibernate-orm.database.generation.halt-on-error`"
msgstr ""

#. type: Plain text
#: upstream/_posts/2020-12-01-quarkus-1-10-2-final-released.adoc:94
msgid "`quarkus.hibernate-orm.log.sql`"
msgstr ""

#. type: Plain text
#: upstream/_posts/2020-12-01-quarkus-1-10-2-final-released.adoc:95
msgid "`quarkus.hibernate-orm.log.format-sql`"
msgstr ""

