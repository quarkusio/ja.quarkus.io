#. type: Plain text
#: upstream/_posts/2020-03-19-eclipse-got-quarkused.adoc:230
msgid "Create a `MyServiceClient.java` file in the org.acme package with the following content:"
msgstr ""

#. type: delimited block -
#: upstream/_posts/2020-03-19-eclipse-got-quarkused.adoc:233
#, no-wrap
msgid "package org.acme;\n"
msgstr ""

#. type: delimited block -
#: upstream/_posts/2020-03-19-eclipse-got-quarkused.adoc:237
#, no-wrap
msgid ""
"import javax.ws.rs.GET;\n"
"import javax.ws.rs.Path;\n"
"import javax.ws.rs.core.Response;\n"
msgstr ""

#. type: delimited block -
#: upstream/_posts/2020-03-19-eclipse-got-quarkused.adoc:239
#, no-wrap
msgid "import org.eclipse.microprofile.rest.client.inject.RegisterRestClient;\n"
msgstr ""

#. type: delimited block -
#: upstream/_posts/2020-03-19-eclipse-got-quarkused.adoc:246
#, no-wrap
msgid ""
"@RegisterRestClient\n"
"public interface MyServiceClient {\n"
"    @GET\n"
"    @Path(\"/greet\")\n"
"    Response greet();\n"
"}\n"
msgstr ""

