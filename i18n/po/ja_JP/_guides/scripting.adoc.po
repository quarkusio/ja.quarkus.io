#. type: Plain text
#: upstream/_guides/scripting.adoc:111
msgid   "The next line"
msgstr  ""

#. type: delimited block -
#: upstream/_guides/scripting.adoc:115
#, no-wrap
msgid   "// //DEPS <dependency1> <dependency2>\n"
msgstr  ""

