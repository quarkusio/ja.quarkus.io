#. type: delimited block -
#: upstream/_guides/security-keycloak-authorization.adoc:119
#, no-wrap
msgid "import io.quarkus.security.identity.SecurityIdentity;\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/security-keycloak-authorization.adoc:122
#, no-wrap
msgid ""
"@Path(\"/api/users\")\n"
"public class UsersResource {\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/security-keycloak-authorization.adoc:125
#: upstream/_guides/security-keycloak-authorization.adoc:316
#, no-wrap
msgid ""
"    @Inject\n"
"    SecurityIdentity identity;\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/security-keycloak-authorization.adoc:132
#, no-wrap
msgid ""
"    @GET\n"
"    @Path(\"/me\")\n"
