#, no-wrap
msgid "quarkus.sns.endpoint-override=http://localhost:8009\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/amazon-sns.adoc:626
#, no-wrap
msgid "quarkus.sns.aws.region=us-east-1\nquarkus.sns.aws.credentials.type=static\nquarkus.sns.aws.credentials.static-provider.access-key-id=test-key\nquarkus.sns.aws.credentials.static-provider.secret-access-key=test-secret\n"
msgstr ""

#. type: Plain text
