msgid "an IDE"
msgstr ""

#. type: Plain text
#: upstream/_guides/jms.adoc:23
msgid "JDK 1.8+ installed with `JAVA_HOME` configured appropriately"
msgstr ""

#. type: Plain text
#: upstream/_guides/jms.adoc:24
msgid "Apache Maven {maven-version}"
msgstr ""

