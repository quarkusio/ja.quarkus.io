#. type: delimited block -
#: upstream/_guides/spring-di.adoc:108
#, no-wrap
msgid ""
"    @Bean(name = \"capitalizeFunction\")\n"
"    public StringFunction capitalizer() {\n"
"        return String::toUpperCase;\n"
"    }\n"
"}\n"
msgstr ""

#. type: Plain text
#: upstream/_guides/spring-di.adoc:113
#, fuzzy
msgid ""
