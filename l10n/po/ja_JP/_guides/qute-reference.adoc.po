#: upstream/_guides/qute-reference.adoc:1666
#, no-wrap
msgid ""
"@TemplateData(target = BigDecimal.class)\n"
"@TemplateData\n"
"class Item {\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/qute-reference.adoc:1680
#, no-wrap
msgid ""
