#. type: delimited block -
#: upstream/_guides/rest-client-reactive.adoc:501
#, no-wrap
msgid ""
"@Path(\"/v2\")\n"
"@RegisterRestClient\n"
"@RegisterClientHeaders(RequestUUIDHeaderFactory.class)\n"
"public interface CountriesService {\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/rest-client-reactive.adoc:505
#, no-wrap
msgid ""
"    @GET\n"
"    @Path(\"/name/{name}\")\n"
"    Set<Country> getByName(@PathParam(\"name\") String name);\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/rest-client-reactive.adoc:510
#, no-wrap
msgid ""
"    @GET\n"
"    @Path(\"/name/{name}\")\n"
"    CompletionStage<Set<Country>> getByNameAsync(@PathParam(\"name\") String name);\n"
"}\n"
msgstr ""

#. type: Plain text
#: upstream/_guides/rest-client-reactive.adoc:513
msgid "And the `RequestUUIDHeaderFactory` would look like:"
msgstr "そして、 `RequestUUIDHeaderFactory` は次のようになります。"

#. type: delimited block -
#: upstream/_guides/rest-client-reactive.adoc:520
#, no-wrap
msgid ""
"import org.eclipse.microprofile.rest.client.ext.ClientHeadersFactory;\n"
"import org.jboss.resteasy.specimpl.MultivaluedMapImpl;\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/rest-client-reactive.adoc:524
#, no-wrap
msgid ""
"import javax.enterprise.context.ApplicationScoped;\n"
"import javax.ws.rs.core.MultivaluedMap;\n"
"import java.util.UUID;\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/rest-client-reactive.adoc:527
#, no-wrap
msgid ""
"@ApplicationScoped\n"
"public class RequestUUIDHeaderFactory implements ClientHeadersFactory {\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/rest-client-reactive.adoc:535
#, no-wrap
msgid ""
"    @Override\n"
"    public MultivaluedMap<String, String> update(MultivaluedMap<String, String> incomingHeaders, MultivaluedMap<String, String> clientOutgoingHeaders) {\n"
"        MultivaluedMap<String, String> result = new MultivaluedMapImpl<>();\n"
