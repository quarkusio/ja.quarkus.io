msgstr ""

#. type: delimited block -
#: upstream/_guides/cassandra.adoc:476
#, no-wrap
msgid ""
"  public Uni<Void> add(Fruit fruit) {\n"
"    return fruitDao.update(fruit);\n"
"  }\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/cassandra.adoc:481
#, no-wrap
