#. type: delimited block -
#: upstream/_guides/mongodb-panache.adoc:438
#, no-wrap
msgid ""
"// get the second page\n"
"List<Person> secondPage = livingPersons.nextPage().list();\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/mongodb-panache.adoc:441
#, no-wrap
msgid ""
"// get page 7\n"
"List<Person> page7 = livingPersons.page(Page.of(7, 25)).list();\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/mongodb-panache.adoc:444
#, no-wrap
msgid ""
"// get the number of pages\n"
