#. type: Title ==
#: upstream/_guides/rest-client.adoc:330
#, no-wrap
msgid "Async Support"
msgstr "非同期サポート"

#. type: Plain text
#: upstream/_guides/rest-client.adoc:334
msgid "The rest client supports asynchronous rest calls.  Async support comes in 2 flavors: you can return a `CompletionStage` or a `Uni` (requires the `quarkus-rest-client-mutiny` extension).  Let's see it in action by adding a `getByNameAsync` method in our `CountriesService` REST interface. The code should look like:"
msgstr "rest クライアントは非同期の rest 呼び出しをサポートしています。非同期のサポートには、 `CompletionStage` と `Uni` ( `quarkus-rest-client-mutiny` のエクステンションが必要です) の 2 種類があります。 `CountriesService` REST インタフェースに `getByNameAsync` メソッドを追加してみましょう。コードは以下のようになります。"

#. type: delimited block -
#: upstream/_guides/rest-client.adoc:347 upstream/_guides/rest-client.adoc:435
#: upstream/_guides/rest-client.adoc:527
#, no-wrap
msgid ""
"import javax.ws.rs.GET;\n"
"import javax.ws.rs.Path;\n"
"import javax.ws.rs.Produces;\n"
"import java.util.concurrent.CompletionStage;\n"
"import java.util.Set;\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/rest-client.adoc:356 upstream/_guides/rest-client.adoc:537
#: upstream/_guides/rest-client.adoc:588
#, no-wrap
msgid ""
"    @GET\n"
"    @Path(\"/name/{name}\")\n"
"    @Produces(\"application/json\")\n"
"    Set<Country> getByName(@PathParam String name);\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/rest-client.adoc:362 upstream/_guides/rest-client.adoc:543
#: upstream/_guides/rest-client.adoc:594
#, no-wrap
msgid ""
"    @GET\n"
"    @Path(\"/name/{name}\")\n"
"    @Produces(\"application/json\")\n"
"    CompletionStage<Set<Country>> getByNameAsync(@PathParam String name);\n"
"}\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/rest-client.adoc:380 upstream/_guides/rest-client.adoc:465
#, no-wrap
msgid ""
"import javax.inject.Inject;\n"
"import javax.ws.rs.GET;\n"
"import javax.ws.rs.Path;\n"
"import javax.ws.rs.Produces;\n"
"import javax.ws.rs.core.MediaType;\n"
"import java.util.concurrent.CompletionStage;\n"
"import java.util.Set;\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/rest-client.adoc:394
#, no-wrap
msgid ""
"    @GET\n"
"    @Path(\"/name/{name}\")\n"
"    public Set<Country> name(@PathParam String name) {\n"
"        return countriesService.getByName(name);\n"
"    }\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/rest-client.adoc:401
#, no-wrap
msgid ""
"    @GET\n"
"    @Path(\"/name-async/{name}\")\n"
"    public CompletionStage<Set<Country>> nameAsync(@PathParam String name) {\n"
"        return countriesService.getByNameAsync(name);\n"
"    }\n"
"}\n"
