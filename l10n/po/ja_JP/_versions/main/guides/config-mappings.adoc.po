#: upstream/_versions/main/guides/config-mappings.adoc:404
#, no-wrap
msgid ""
"    Map<String, String> form();\n"
"}\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/config-mappings.adoc:414
#, no-wrap
