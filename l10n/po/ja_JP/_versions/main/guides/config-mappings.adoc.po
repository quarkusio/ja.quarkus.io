#~ msgid ""
#~ "@ApplicationScoped\n"
#~ "public class SomeBean {\n"
#~ msgstr ""
#~ "@ApplicationScoped\n"
#~ "public class SomeBean {\n"

#~ msgid ""
#~ "    @Inject <1>\n"
#~ "    GreetingConfiguration greetingConfiguration;\n"
#~ msgstr ""
#~ "    @Inject <1>\n"
#~ "    GreetingConfiguration greetingConfiguration;\n"

#~ msgid ""
#~ "    @ConfigPrefix(\"other\") <2>\n"
#~ "    GreetingConfiguration otherConfiguration;\n"
#~ msgstr ""
