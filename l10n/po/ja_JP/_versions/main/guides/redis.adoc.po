#: upstream/_versions/main/guides/redis.adoc:135
msgid "Create the `src/main/java/org/acme/redis/IncrementService.java` file, with the following content:"
msgstr "以下の内容の `src/main/java/org/acme/redis/IncrementService.java` ファイルを作成します。"

#. type: delimited block -
#: upstream/_versions/main/guides/redis.adoc:143
#, no-wrap
msgid ""
"import io.quarkus.redis.client.RedisClient;\n"
"import io.quarkus.redis.client.reactive.ReactiveRedisClient;\n"
"import io.smallrye.mutiny.Uni;\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/redis.adoc:145
#, no-wrap
msgid "import io.vertx.mutiny.redis.client.Response;\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/redis.adoc:149
#, no-wrap
msgid ""
"import java.util.ArrayList;\n"
"import java.util.Arrays;\n"
"import java.util.List;\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/redis.adoc:152
#, no-wrap
msgid ""
"import javax.inject.Inject;\n"
"import javax.inject.Singleton;\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/redis.adoc:155
#, no-wrap
msgid ""
"@Singleton\n"
"class IncrementService {\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/redis.adoc:158
#, no-wrap
msgid ""
"    @Inject\n"
"    RedisClient redisClient; // <1>\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/redis.adoc:161
#, no-wrap
msgid ""
"    @Inject\n"
"    ReactiveRedisClient reactiveRedisClient; // <2>\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/redis.adoc:166
#, no-wrap
msgid ""
"    Uni<Void> del(String key) {\n"
"        return reactiveRedisClient.del(Arrays.asList(key))\n"
"                .map(response -> null);\n"
"    }\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/redis.adoc:170
#, no-wrap
msgid ""
"    String get(String key) {\n"
"        return redisClient.get(key).toString();\n"
"    }\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/redis.adoc:174
#, no-wrap
msgid ""
"    void set(String key, Integer value) {\n"
"        redisClient.set(Arrays.asList(key, value.toString()));\n"
"    }\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/redis.adoc:178
#, no-wrap
msgid ""
"    void increment(String key, Integer incrementBy) {\n"
"        redisClient.incrby(key, incrementBy.toString());\n"
"    }\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/redis.adoc:191
#, no-wrap
msgid ""
"    Uni<List<String>> keys() {\n"
"        return reactiveRedisClient\n"
"                .keys(\"*\")\n"
"                .map(response -> {\n"
"                    List<String> result = new ArrayList<>();\n"
"                    for (Response r : response) {\n"
"                        result.add(r.toString());\n"
"                    }\n"
"                    return result;\n"
"                });\n"
"    }\n"
"}\n"
msgstr ""

