#. type: delimited block -
#: upstream/_versions/main/guides/security-jwt.adoc:75
#: upstream/_versions/main/guides/security-jwt.adoc:99
#: upstream/_versions/main/guides/security-jwt.adoc:213
#: upstream/_versions/main/guides/security-jwt.adoc:353
#: upstream/_versions/main/guides/security-jwt.adoc:498
#, no-wrap
msgid "package org.acme.security.jwt;\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/main/guides/security-jwt.adoc:80
#, no-wrap
msgid ""
"import javax.ws.rs.GET;\n"
"import javax.ws.rs.Path;\n"
