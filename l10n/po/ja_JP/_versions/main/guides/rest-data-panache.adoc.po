"[\n"
"  {\n"
"    \"id\": 1,\n"
"    \"name\": \"John Johnson\",\n"
"    \"birth\": \"1988-01-10\"\n"
"  },\n"
"  {\n"
"    \"id\": 2,\n"
"    \"name\": \"Peter Peterson\",\n"
"    \"birth\": \"1986-11-20\"\n"
"  }\n"
"]\n"
