#, no-wrap
msgid "Creating the Increment Resource"
msgstr "インクリメントリソースの作成"

#. type: Plain text
#: upstream/_versions/2.13/guides/redis.adoc:175
msgid "Create the `src/main/java/org/acme/redis/IncrementResource.java` file, with the following content:"
msgstr "以下の内容の `src/main/java/org/acme/redis/IncrementResource.java` ファイルを作成します:"

#. type: delimited block -
#: upstream/_versions/2.13/guides/redis.adoc:187
#, no-wrap
msgid ""
"import javax.inject.Inject;\n"
"import javax.ws.rs.GET;\n"
"import javax.ws.rs.PUT;\n"
"import javax.ws.rs.Path;\n"
"import javax.ws.rs.POST;\n"
"import javax.ws.rs.DELETE;\n"
"import java.util.List;\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/2.13/guides/redis.adoc:189
#, no-wrap
msgid "import io.smallrye.mutiny.Uni;\n"
msgstr "import io.smallrye.mutiny.Uni;\n"

#. type: delimited block -
#: upstream/_versions/2.13/guides/redis.adoc:192
#, no-wrap
msgid ""
"@Path(\"/increments\")\n"
"public class IncrementResource {\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/2.13/guides/redis.adoc:195
#, no-wrap
msgid ""
"    @Inject\n"
"    IncrementService service;\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/2.13/guides/redis.adoc:200
#, no-wrap
msgid ""
"    @GET\n"
"    public Uni<List<String>> keys() {\n"
"        return service.keys();\n"
"    }\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/2.13/guides/redis.adoc:206
#, no-wrap
msgid ""
"    @POST\n"
"    public Increment create(Increment increment) {\n"
"        service.set(increment.key, increment.value);\n"
"        return increment;\n"
"    }\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/2.13/guides/redis.adoc:212
#, no-wrap
msgid ""
"    @GET\n"
"    @Path(\"/{key}\")\n"
"    public Increment get(String key) {\n"
"        return new Increment(key, service.get(key));\n"
"    }\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/2.13/guides/redis.adoc:218
#, no-wrap
msgid ""
"    @PUT\n"
"    @Path(\"/{key}\")\n"
"    public void increment(String key, long value) {\n"
"        service.increment(key, value);\n"
"    }\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/2.13/guides/redis.adoc:225
#, no-wrap
msgid ""
"    @DELETE\n"
"    @Path(\"/{key}\")\n"
"    public Uni<Void> delete(String key) {\n"
"        return service.del(key);\n"
"    }\n"
"}\n"
msgstr ""

