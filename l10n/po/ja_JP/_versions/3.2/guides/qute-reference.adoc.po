#: upstream/_versions/3.2/guides/qute-reference.adoc:958
#, no-wrap
msgid "not in"
msgstr ""

#. type: Table
#: upstream/_versions/3.2/guides/qute-reference.adoc:959
#, no-wrap
msgid "`ni`,`!in`"
msgstr ""

