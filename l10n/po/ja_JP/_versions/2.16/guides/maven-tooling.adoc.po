msgstr ""

#. type: Plain text
#: upstream/_versions/2.16/guides/maven-tooling.adoc:469
msgid "If you have not used <<project-creation,project scaffolding>>, add the following elements in your `pom.xml`"
msgstr ""

#. type: delimited block -
#: upstream/_versions/2.16/guides/maven-tooling.adoc:475
#, no-wrap
msgid ""
"<properties>\n"
"    <skipITs>true</skipITs> <1>\n"
"</properties>\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/2.16/guides/maven-tooling.adoc:487
#, no-wrap
msgid ""
"<dependencyManagement>\n"
"    <dependencies>\n"
"        <dependency> <2>\n"
"            <groupId>${quarkus.platform.group-id}</groupId>\n"
"            <artifactId>quarkus-bom</artifactId>\n"
"            <version>${quarkus.platform.version}</version>\n"
"            <type>pom</type>\n"
"            <scope>import</scope>\n"
"        </dependency>\n"
"    </dependencies>\n"
"</dependencyManagement>\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/2.16/guides/maven-tooling.adoc:538
#, no-wrap
msgid ""
"<build>\n"
"    <plugins>\n"
"        <plugin> <3>\n"
"            <groupId>${quarkus.platform.group-id}</groupId>\n"
"            <artifactId>quarkus-maven-plugin</artifactId>\n"
"            <version>${quarkus.platform.version}</version>\n"
"            <extensions>true</extensions> <4>\n"
"            <executions>\n"
"                <execution>\n"
"                    <goals>\n"
