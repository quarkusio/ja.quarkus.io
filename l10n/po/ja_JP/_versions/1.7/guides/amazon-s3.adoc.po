#, no-wrap
msgid ""
"    protected File tempFilePath() {\n"
"        return new File(TEMP_DIR, new StringBuilder().append(\"s3AsyncDownloadedTemp\")\n"
"                .append((new Date()).getTime()).append(UUID.randomUUID())\n"
"                .append(\".\").append(\".tmp\").toString());\n"
"    }\n"
