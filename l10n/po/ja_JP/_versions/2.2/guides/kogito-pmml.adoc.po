"    \"correlationId\": null,\n"
"    \"segmentationId\": null,\n"
"    \"segmentId\": null,\n"
"    \"segmentIndex\": 0,\n"
"    \"resultCode\": \"OK\",\n"
"    \"resultObjectName\": \"Species\",\n"
"    \"resultVariables\": {\n"
"        \"Probability_setosa\": 0.04871813160275851,\n"
"        \"Probability_versicolor\": 0.04509592640753013,\n"
"        \"Probability_virginica\": 0.9061859419897114,\n"
"        \"Species\": \"virginica\"\n"
"    }\n"
"}\n"
