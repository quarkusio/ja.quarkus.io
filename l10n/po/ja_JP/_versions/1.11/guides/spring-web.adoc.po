"}\n"
msgstr ""
"    @GetMapping\n"
"    public String hello() {\n"
"        return \"hello\";\n"
"    }\n"
"}\n"

#. type: Title ==
#: upstream/_versions/1.11/guides/spring-web.adoc:95
#, no-wrap
msgid "GreetingControllerTest"
msgstr "GreetingControllerTest"

