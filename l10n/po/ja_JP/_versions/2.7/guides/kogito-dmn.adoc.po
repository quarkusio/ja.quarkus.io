msgid ""
"curl -X POST 'http://localhost:8080/pricing' \\\n"
"-H 'Accept: application/json' \\\n"
"-H 'Content-Type: application/json' \\\n"
"-d '{ \"Age\": 47, \"Previous incidents?\": false }'\n"
