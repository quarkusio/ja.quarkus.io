#: upstream/_versions/3.0/guides/cache.adoc:552
#: upstream/_versions/3.0/guides/cache.adoc:938
#, no-wrap
msgid ""
"@ApplicationScoped\n"
"public class CacheClearer {\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/cache.adoc:555
#: upstream/_versions/3.0/guides/cache.adoc:686
#: upstream/_versions/3.0/guides/cache.adoc:941
#, no-wrap
msgid ""
"    @Inject\n"
"    CacheManager cacheManager;\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/3.0/guides/cache.adoc:563
